@model IEnumerable<SignUpUserVM>
@{
    ViewData["Title"] = "Index";
    int counter = 1;
}

<div class="ContentBody">
    <section class="py-5">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            All User
                        </div>
                        <div class="card-body">
                            <table class="table ExSTable ExSDataTable" id="UserTab">
                                <thead class="thead-dark">
                                    <tr>
                                        <th scope="col">#</th>
                                        <th scope="col">First Name</th>
                                        <th scope="col">Last Name</th>
                                        <th scope="col">Email</th>
                                        <th scope="col">Phone Number</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <th scope="row">@counter</th>
                                            <td>@item.FirstName</td>
                                            <td>@item.LastName</td>
                                            <td>@item.Email</td>
                                            <td>@item.PhoneNumber</td>
                                        </tr>
                                        counter++;
                                    }
                                </tbody>
                            </table>
                            <p id="heading"></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>


