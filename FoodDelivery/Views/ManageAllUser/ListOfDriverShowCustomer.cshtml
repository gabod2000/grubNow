
@model IEnumerable<SignUpDriverVM>

@{
   Layout = "~/Views/Shared/_LayoutPublicForTab.cshtml";
}

@foreach (var course in Model)
{
    <div class="card mb-4" style="z-index:-1">
        <div class="card-body">
            <h2>Driver</h2>
            <div class="row">
                <div class="col-md-4">
                    @if (!string.IsNullOrEmpty(course.ProfilePic))
                    {
                        <img id="imagePreviewTec" style="border-radius:100px" height="200" width="200" src="~/UploadedFiles/@course.ProfilePic" />
                    }
                    else
                    {
                        <img id="imagePreviewTec" style="border-radius:100px" height="200" width="200" src="data:image/png;base64," />
                        <span class="ml-3">No Profile Picture</span>
                     }
                </div>
                <div class="col-md-4 border-left">
                    <h3 class="card-title">Driver Details</h3>
                    <label>Name  : </label> <label>@course.FirstName  @course.LastName</label><br />
                    <label>Phone Number  : </label> <label>@course.PhoneNumber</label><br />
                    <label>Email  : </label>  <label>@course.Email</label>
                </div>
                <div class="col-md-4 border-left">
                    <div class="avatar-preview">
                        @if (!string.IsNullOrEmpty(course.CarPic))
                        {
                            <img id="imagePreviewTec1" style="border-radius:100px" height="200" width="200" src="~/UploadedFiles/@course.CarPic" />
                        }
                        else
                        {
                            <img id="imagePreviewTec1" style="border-radius:100px" height="200" width="200" src="data:image/jpeg;base64," />
                            <span class="ml-3">No Picture</span>
                        }
                    </div>
                    </div>
                </div>
        </div>
    </div>
}


<script>
        //$(".header_text_div").addClass("d-none");
        $('#CheckBoxConfirm').click(function () {
            if ($(this).prop("checked") == true) {
                $("#btnNext").removeClass("isDisabled");
            }
            else if ($(this).prop("checked") == false) {
                $("#btnNext").addClass("isDisabled");

            }
        });

</script>
