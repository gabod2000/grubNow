@model IEnumerable<AreaListVM>

@{
    Layout = null;
}


    <div class="row mt-3">
        @foreach (var course in Model)
        {
            if (course.Name != "Other")
            {
                <div class="col-lg-3 col-md-4 col-6 mb-4 AreaName">
                    <a href="#" class="text-left" style=" text-decoration: none;color: black;">@course.Name</a>
                </div>
            }
        }
    </div>
